module WhopSDK
  module Models
    type refund_status =
      :pending | :requires_action | :succeeded | :failed | :canceled

    module RefundStatus
      extend WhopSDK::Internal::Type::Enum

      PENDING: :pending
      REQUIRES_ACTION: :requires_action
      SUCCEEDED: :succeeded
      FAILED: :failed
      CANCELED: :canceled

      def self?.values: -> ::Array[WhopSDK::Models::refund_status]
    end
  end
end
